{
  "name": "jupiter-platform",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=8.19.4"
  },
  "scripts": {
    "build": "nx run-many --target=build --all",
    "build:framework": "nx build worker-framework",
    "build:email": "nx build email-service --with-deps",
    "build:scraper-anses": "nx build scraper-anses --with-deps",
    "serve:email": "node dist/apps/email-service/main.js",
    "serve:scraper-anses": "node dist/apps/scraper-anses/main.js",
    "build:worker": "nx build",
    "serve:worker": "nx serve",
    "lint:worker": "nx lint",
    "test": "nx run-many --target=test --all",
    "test:framework": "nx test worker-framework",
    "test:email": "nx test email-service",
    "test:scraper-anses": "nx test scraper-anses",
    "lint": "nx run-many --target=lint --all",
    "graph": "nx graph",
    "affected": "nx affected",
    "affected:build": "nx affected --target=build",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "affected:lint": "nx affected --target=lint",
    "affected:lint:fix": "nx affected --target=lint --fix",
    "affected:test": "nx affected --target=test --passWithNoTests",
    "pre-push": "node ./tools/scripts/pre-push-checks.js",
    "ci:lint": "nx affected --target=lint --configuration=ci",
    "ci:test": "nx affected --target=test --passWithNoTests --configuration=ci",
    "ci:build": "nx affected --target=build --configuration=production",
    "verify": "npm run affected:lint:fix && npm run affected:test && npm run affected:build"
  },
  "private": true,
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nx/devkit": "^20.5.0",
    "@nx/esbuild": "20.4.6",
    "@nx/eslint": "20.4.6",
    "@nx/eslint-plugin": "20.4.6",
    "@nx/jest": "20.4.6",
    "@nx/js": "^20.4.6",
    "@nx/node": "^20.4.6",
    "@nx/plugin": "^20.4.6",
    "@nx/workspace": "20.4.6",
    "@swc-node/register": "^1.10.9",
    "@swc/core": "^1.11.8",
    "@types/amqplib": "^0.10.7",
    "@types/dotenv": "^6.1.1",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/node": "22.13.1",
    "@types/nodemailer": "^6.4.17",
    "@types/python-shell": "^0.4.2",
    "esbuild": "0.19.8",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.0.0",
    "fs-extra": "^11.3.0",
    "haiku-generator": "github:garciafido/haiku-generator#main",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "js-yaml": "^4.1.0",
    "nx": "^20.4.6",
    "prettier": "^2.8.8",
    "prisma": "^6.4.1",
    "simple-git": "^3.27.0",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tslib": "^2.8.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.19.0",
    "verdaccio": "^5.0.4",
    "yaml": "^2.7.0"
  },
  "overrides": {
    "esbuild": "0.19.8"
  },
  "dependencies": {
    "@apollo/client": "^3.13.1",
    "@apollo/server": "^4.11.3",
    "@prisma/client": "^6.4.1",
    "@types/prettier": "^2.7.3",
    "amqplib": "^0.10.5",
    "cross-fetch": "^4.1.0",
    "dotenv": "^16.4.7",
    "graphql": "^16.10.0",
    "lodash": "^4.17.21",
    "nodemailer": "^6.10.0",
    "playwright": "^1.50.1",
    "python-shell": "^5.0.0",
    "shelljs": "^0.8.5",
    "winston": "^3.17.0"
  },
  "nx": {
    "includedScripts": []
  },
  "generators": "./generators.json"
}
